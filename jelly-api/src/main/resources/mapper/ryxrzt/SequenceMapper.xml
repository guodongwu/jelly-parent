<mapper namespace="com.ryx.ryxrzt.mapper.SequenceMapper">

	<select id="selectMenuSeq"  resultType="java.lang.String">
		select menu_seq.nextval as value from dual
	</select>
	
	<select id="selectUserOrgSeq"  resultType="java.lang.String">
		select user_org_seq.nextval as value from dual
	</select>
	
	<select id="selectRoleSeq"  resultType="java.lang.String">
		select user_role_seq.nextval as value from dual
	</select>
	
	
	<select id="selectUserInfoSeq"  resultType="java.lang.String">
		select user_info_seq.nextval as value from dual
	</select>
	
	
</mapper>